{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s --copy-files",
    "build": "npm run clean && npm run build-babel",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "start": "npm run build && node ./build/index.js",
    "server:dev": "nodemon --inspect --ignore client/ --exec babel-node -- server/index.js",
    "client:dev": "npm start --prefix client",
    "webhook": "lt -p 5000 -s maileraj1234",
    "dev": "concurrently --kill-others-on-fail \"npm run server:dev\" \"wait-on tcp:5000 && npm run client:dev\" \"npm run webhook\""
  },
  "engines": {
    "node": "8.9.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.6",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pug": "^2.0.3",
    "stripe": "^6.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "localtunnel": "^1.9.0",
    "nodemon": "^1.18.3",
    "wait-on": "^2.1.0"
  }
}
